{
  "name": "deckdeckgo-docs",
  "version": "1.0.0-alpha.0",
  "description": "Add a lightweight presentation to your web project using HTML and Web Components",
  "license": "MIT",
  "files": [
    "dist/"
  ],
  "scripts": {
    "docs": "tsc --p ./scripts && node ./.deckgo/markdown-to-html",
    "build": "npm run docs && stencil build",
    "start": "npm run docs && stencil build --dev --watch --serve",
    "test": "npm run docs && stencil test --spec --e2e",
    "test.watch": "npm run docs && stencil test --spec --e2e --watch"
  },
  "dependencies": {
    "@ionic/core": "4.0.0",
    "deckdeckgo-highlight-code": "^1.0.0-alpha.7",
    "rxjs": "^6.4.0"
  },
  "devDependencies": {
    "@stencil/core": "0.17.0",
    "@stencil/postcss": "^0.1.0",
    "@stencil/sass": "^0.1.1",
    "@types/glob": "^7.1.1",
    "@types/marked": "^0.6.0",
    "@types/node": "^10.12.20",
    "autoprefixer": "^9.4.6",
    "glob": "^7.1.3",
    "marked": "^0.6.0",
    "workbox-build": "3.4.1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/deckgo/deckdeckgo-docs.git"
  },
  "keywords": [
    "ionic",
    "stencil",
    "stenciljs",
    "web components",
    "pwa",
    "progressive web app",
    "presentation",
    "slides",
    "slideshow",
    "talk"
  ]
}
